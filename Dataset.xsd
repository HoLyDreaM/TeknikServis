            <MainSource>
              <DbSource ConnectionRef="conTS (Settings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Fill" GenerateShortCommands="false" GeneratorSourceName="Fill" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="true" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT t1.ID,
       replace(str(CAST(t1.EvrakNo AS NVARCHAR),6),space(1),'0') AS EvrakNo,     
       CASE t1.HareketDurum WHEN 0 THEN 'Devam Ediyor' WHEN 1 THEN 'Bitmiş' END AS HareketDurumu, 
       t1.Aciklama,
       t1.EvrakTarihi,
       t1.MusteriKodu,
       c.Unvan1, 
       t1.BaslangicTarih,
       t1.BitisTarihi,
       t1.PeriyotAraligi,
       CAST(t1.PeriyotAraligi AS NVARCHAR) + ' Ay ara ile' AS PeriyotAraligiStr,
       (SELECT COUNT(d.BakimEvrakNo) FROM tblBakimAnlasmaDetayi d WHERE d.BakimEvrakNo = t1.EvrakNo) AS ToplamKacAy,
       (SELECT COUNT(d.BakimEvrakNo) FROM tblBakimAnlasmaDetayi d WHERE d.BakimEvrakNo = t1.EvrakNo AND d.HareketDurumu = 1) AS Gidilen,
       ((SELECT COUNT(d.BakimEvrakNo) FROM tblBakimAnlasmaDetayi d WHERE d.BakimEvrakNo = t1.EvrakNo) - 
       (SELECT COUNT(d.BakimEvrakNo) FROM tblBakimAnlasmaDetayi d WHERE d.BakimEvrakNo = t1.EvrakNo AND d.HareketDurumu = 1)) AS Kalan,
       t1.BakimBilgisi,
       t1.Notlar
FROM   tblBakimAnlasmalari t1
       INNER JOIN CariTanimlari c
            ON  t1.MusteriKodu = c.HesapKodu</CommandText>
